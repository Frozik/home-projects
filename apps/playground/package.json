{
  "name": "@frozik/playground",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "serve": "vite",
    "build": "tsc -b && vite build",
    "lint": "TIMING=1 eslint . --report-unused-disable-directives --max-warnings 0",
