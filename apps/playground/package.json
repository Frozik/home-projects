    "@react-three/fiber": "^8.17.12",
    "@redux-saga/core": "^1.3.0",
    "@reduxjs/toolkit": "^2.5.0",
    "@tensorflow/tfjs": "^4.22.0",
    "@tensorflow/tfjs-core": "^4.22.0",
    "antd": "^5.23.0",
    "classnames": "^2.5.1",
    "dockview": "^3.0.2",
    "idb": "^8.0.1",
    "matter-js": "^0.20.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.2.0",
